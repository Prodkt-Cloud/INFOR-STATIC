---
import AsideRadioElements from '../organisms/AsideRadioElements.astro'

const response = await fetch("http://127.0.0.1:1337/api/learning-roles?populate=*");
const data = await response.json();

---

{
    data.map((learnerRoles) => (
      <>
        <AsideRadioElements {learnerRoles.id.attributes.title} />
      </>
    ))
  }