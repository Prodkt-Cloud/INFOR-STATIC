{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "moduleResolution": "nodenext",
    "baseUrl": ".",
    "strictNullChecks": true,
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["public/*"],
      "@pagefind/*": [".vercel/output/static/*"],
      "~/*": ["src/*"]
    }
  }
}